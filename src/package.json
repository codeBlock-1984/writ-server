{
  "name": "writ-server",
  "version": "1.0.0",
  "description": "API server for the writ app.",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "serve": "nodemon --exec babel-node ./src/server.js",
